# Configuration du serveur
PORT=3000
NODE_ENV=development

# Cle API pour securiser l'acces (generer avec: openssl rand -hex 32)
API_KEY=votre_cle_api_secrete_changez_moi

# Identifiants Chronopost
# TEST: Utilisez les identifiants de test Chronopost pour le developpement
# PRODUCTION: Remplacez par vos identifiants de production apres validation
CHRONOPOST_ACCOUNT_NUMBER=votre_numero_compte
CHRONOPOST_PASSWORD=votre_mot_de_passe
CHRONOPOST_WSDL_URL=https://ws.chronopost.fr/shipping-cxf/ShippingServiceWS?wsdl

# Informations de votre entreprise (expediteur)
SHIPPER_NAME=Votre Entreprise
SHIPPER_ADDRESS1=123 Rue Example
SHIPPER_ADDRESS2=
SHIPPER_CITY=Paris
SHIPPER_ZIPCODE=75001
SHIPPER_COUNTRY=FR
SHIPPER_EMAIL=contact@votre-entreprise.fr
SHIPPER_PHONE=0102030405
SHIPPER_CONTACT_NAME=Service Expedition
